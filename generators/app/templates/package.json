{
  "name": "@<%= answers['module:scope'] %>/<%= slug %>",
  "description": "<%= answers['module:description'] %>",
  "main": "dist/index.js",
  "jsnext:main": "dist/index.es2015.js",
  "version": "0.0.0-semantically-released",
  "scripts": {
    "commit": "git-cz",
    "lint": "eslint *.js __test__/ src/ ",
    "lint:fix": "npm run lint -- --fix",
    "build": "NODE_ENV=production gulp sass-build && npm run build-cjs && npm run build-es6",
    "build-cjs": "rimraf lib && BABEL_ENV=production babel ./src -d lib --plugins=transform-es2015-modules-commonjs",
    "build-es6": "rimraf es6 && babel ./src -d es6",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test:watch": "npm t -- -w",
    "test": "mocha test/unit test/functional  --compilers js:babel-register",
    "test:karma": "eslint src/ spec/ test/ && ./node_modules/karma/bin/karma start karma.conf.js",
    "test:karma:watch": "./node_modules/karma/bin/karma start karma.conf.js --auto-watch --no-single-run",
    "cover": "nyc npm t",
    "prebuild": "rimraf dist",
    "build": "babel --copy-files --out-dir lib --ignore test src",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git"
  },
  "keywords": [
    "react",
    "reactjs",
    "react-component",
    "es6"
  ],
  "author": {
    "name": "<%= answers['module:author:fullName'] %>",
    "email": "<%= answers['module:author:email'] %>"
  },
  "license": "<%= answers['license']%>",
  "dependencies": {
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "react": "^15.5.4",
    "react-addons-css-transition-group": "^15.5.2",
    "react-addons-transition-group": "^15.5.2",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "reactstrap": "^4.5.0",
    "recharts": "^0.22.4",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-react-router": "^1.0.0-beta3",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "*",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.24.1",
    "chai": "3.5.0",
    "commitizen": "^2.9.6",
    "coveralls": "*",
    "css-loader": "^0.28.1",
    "cz-conventional-changelog": "^2.0.0",
    "es5-shim": "^4.5.9",
    "eslint": "^3.13.1",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-babel": "^4.1.0",
    "eslint-plugin-react": "^7.0.0",
    "exports-loader": "^0.6.4",
    "express": "^4.15.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "ghooks": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-clean": "^0.3.2",
    "gulp-connect": "^5.0.0",
    "gulp-data": "^1.2.1",
    "gulp-debug": "^3.0.0",
    "gulp-inject": "^4.2.0",
    "gulp-json-srv": "^1.0.1",
    "gulp-load-plugins": "^1.2.2",
    "gulp-minify-css": "^1.2.4",
    "gulp-nunjucks": "^3.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-rimraf": "^0.2.1",
    "gulp-sass": "^3.1.0",
    "gulp-util": "^3.0.8",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.26.0",
    "jasmine": "^2.6.0",
    "jasmine-core": "^2.6.1",
    "jest": "^20.0.0",
    "jest-cli": "^20.0.0",
    "jest-junit": "^1.0.4",
    "karma": "^1.5.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-mocha": "^1.1.1",
    "karma-nested-reporter": "^0.1.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.1",
    "mocha": "^3.0.2",
    "node-sass": "^4.3.0",
    "nyc": "^10.3.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "semantic-release": "^6.3.6",
    "sinon": "*",
    "sinon-chai": "*",
    "style-loader": "^0.17.0",
    "webpack": "^2.2.1",
    "webpack-bundle-tracker": "^0.2.0",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-manifest-plugin": "^1.1.0",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.5.4",
    "webpack-stream": "^3.2.0",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test"
    }
  }
}
