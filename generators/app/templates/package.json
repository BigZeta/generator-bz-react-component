{
  "name": "@<%= answers['module:scope'] %>/<%= answers['module:name'] %>",
  "description": "<%= answers['module:description'] %>",
  "main": "dist/index.js",
  "jsnext:main": "dist/index.es2015.js",
  "version": "0.0.0-semantically-released",
  "scripts": {
    "commit": "git-cz",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test:watch": "npm t -- -w",
    "test": "mocha test/unit test/functional  --compilers js:babel-register",
    "cover": "nyc npm t",
    "prebuild": "rimraf dist",
    "build": "babel --copy-files --out-dir dist --ignore test src",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "keywords": [
    "bigzeta",
    "npm",
    "es6"
  ],
  "author": {
    "name": "<%= answers['module:author:nickname'] %>",
    "email": "<%= answers['module:author:email'] %>",
    "url": "https://github.com/<%= answers['module:author:github'] %>"
  },
  "license": "<%= answers['license'] %>",
  "dependencies": {
  },
  "devDependencies": {
    "babel-cli": "*",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.9.0",
    "chai": "3.5.0",
    "commitizen": "2.8.2",
    "coveralls": "*",
    "cz-conventional-changelog": "1.1.6",
    "ghooks": "1.2.4",
    "mocha": "2.5.3",
    "nyc": "6.6.1",
    "rimraf": "2.5.2",
    "semantic-release": "^4.3.5",
    "sinon": "*",
    "sinon-chai": "*"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/<%= answers['module:scope'] %>/<%= answers['module:name'] %>.git"
  }
}
